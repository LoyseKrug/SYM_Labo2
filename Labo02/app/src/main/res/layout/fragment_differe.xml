<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.sym.labo02.DiffereFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent" tools:layout_editor_absoluteY="122dp"
            tools:layout_editor_absoluteX="0dp">
        <TextView
                android:text="Envoi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/sendLabel"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Données à envoyer"
                android:ems="10"
                android:id="@+id/sendTextfield"/>
        <TextView
                android:text="Logs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/logLabel"/>
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Logs"
                android:ems="10"
                android:id="@+id/logTextfield"/>
        <Button
                android:text="Envoie Différé"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/sendBtn"/>
    </LinearLayout>
</FrameLayout>
